{
    "name": "norman-huth/nova-bbcode-textarea",
    "description": "Textarea with BBCodes. A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/Muetze42/nova-bbcode-textarea",
    "authors": [
        {
            "name": "Norman Huth",
            "email": "imprint-28847@huth.it",
            "homepage": "https://huth.it"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "NormanHuth\\BBCode\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NormanHuth\\BBCode\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
